<h1>テスト</h1>

<div class="container">
  <div class="row">
    <div class="col">
      <%= form_with model: @event do |f| %>
        <div class="title">
          <%= f.label :title %>
          <%= f.text_field :title %>
        </div>
        <div class="time">
          <%= f.label :start_time %>
          <%= f.datetime_select :start_time %>
        </div>
        <div class="time">
          <%= f.label :end_time %>
          <%= f.datetime_select :end_time %>
        </div>
        <div class="content">
          <%= f.label :content %>
          <%= f.text_field :content %>
        </div>
        <div class="submit">
          <%= f.submit '登録' %>
        </div>
      <% end %>
    </div>
  </div>
</div>